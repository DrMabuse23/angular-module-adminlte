{
	"compilerOptions": {
		"baseUrl": "",
		"declaration": true,
		"stripInternal": true,
		"experimentalDecorators": true,
		"strictNullChecks": false,
		"noImplicitAny": false,
		"module": "es2015",
		"moduleResolution": "node",
		"paths": {
			"@angular/core": [
				"node_modules/@angular/core"
			],
			"@angular/common": [
				"node_modules/@angular/common"
			],
			"rxjs/*": [
				"node_modules/rxjs/*"
			],
			"admin-lte/*": [
				"node_modules/admin-lte/*"
			],
			"bootstrap/*": [
				"node_modules/bootstrap/*"
			],
			"font-awesome/*": [
				"node_modules/font-awesome/*"
			],
			"ionicons/*": [
				"node_modules/ionicons/*"
			],
			"core-js/*": [
				"node_modules/core-js/*"
			],
			
			"angular2-toaster": [
				"node_modules/angular2-toaster/*"
			]
		},
		
		"outDir": "dist",
		"sourceMap": true,
		"inlineSources": true,
		"target": "es5",
		"skipLibCheck": true,
		"lib": [
			"es2015",
			"dom"
		]
	},
	"files": [
		"./index.ts"
	],
	"angularCompilerOptions": {
		"strictMetadataEmit": true,
		"genDir": ".tmp"
	}
}